{
  "name": "ts-node-template",
  "private": true,
  "description": "Template for a server written in TypeScript with no compilation step",
  "main": "src/main.ts",
  "scripts": {
    "start": "ts-node -P ./tsconfig.json src/main",
    "watch": "LOG_PRETTY=true LOG_LEVEL=debug ts-node-dev -P ./tsconfig.json src/main",
    "postgres-init": "initdb -D ./data",
    "postgres-start": "pg_ctl -D ./data start",
    "postgres-stop": "pg_ctl -D ./data stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kinrany/ts-node-template.git"
  },
  "author": "kinrany",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/Kinrany/ts-node-template/issues"
  },
  "homepage": "https://github.com/Kinrany/ts-node-template#readme",
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@types/pg": "^7.14.4",
    "@types/pino": "^6.3.0",
    "@types/ramda": "^0.27.14",
    "@types/uuid": "^8.3.0",
    "pino-pretty": "^4.1.0",
    "ramda": "^0.27.1",
    "ts-node-dev": "^1.0.0-pre.60",
    "typescript": "^4.1.0-dev.20200823"
  },
  "dependencies": {
    "fastify": "^3.2.1",
    "fastify-basic-auth": "^1.0.1",
    "fastify-cors": "^4.1.0",
    "fp-ts": "^2.8.1",
    "io-ts": "^2.2.10",
    "io-ts-reporters": "^1.2.2",
    "io-ts-types": "^0.5.10",
    "knex": "^0.21.5",
    "monocle-ts": "^2.3.3",
    "newtype-ts": "^0.3.4",
    "pg": "^8.3.2",
    "pino": "^6.5.1",
    "ramda": "^0.27.1",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.1",
    "uuid": "^8.3.0"
  }
}
